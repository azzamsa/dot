unmapAll

#
# Navigating to new pages
#

map f LinkHints.activateMode
map F LinkHints.activateModeToOpenInNewTab
map <a-f> LinkHints.activateModeWithQueue

#
# Navigating the current page
#

map e scrollPageDown
map E scrollToBottom

map u scrollPageUp
map U scrollToTop

map n previousTab
map i nextTab

#
# Navigating the history
#

map h goBack
map H goForward

#
# Manipulating tabs
#

map x removeTab
map X restoreTab

map t createTab
map d duplicateTab
map r reload
map <a-p> togglePinTab
map <a-m> toggleMuteTab

map << moveTabLeft
map >> moveTabRight

map g1 firstTab
map g0 lastTab

map / visitPreviousTab

map k closeTabsOnRight
map K closeOtherTabs

#
# Misc
#

map ? showHelp