!!! Xresources

!! XFT
Xft.lcdfilter : lcddefault
Xft.hintstyle : hintslight
Xft.hinting   : true
Xft.antialias : true
Xft.rgba      : rgb

!! rofi -- dmenu clone
rofi.opacity:  100
rofi.width:    70
rofi.lines:    15
rofi.font:     Fira Sans, FontAwesome 11
rofi.location: 0
rofi.padding:  5

rofi.color-enabled: true
!                  bg,      border,  header border
rofi.color-window: #F9FAFB, #4338CA, #4338CA

! State:           bg,      fg,      bgalt    hlbg,    hlfg
rofi.color-normal: #F9FAFB, #4338CA, #e0e1e1, #4338CA, #F9FAFB
!                  blue-4,  fg,      blue-5,  blue-3,  fg+1
rofi.color-active: #4c7073, #dcdccc, #366060, #5c888b, #ffffef
!                  red-4,   fg,      red-5,   red-3,   fg+1
rofi.color-urgent: #8c5353, #dcdccc, #7c4343, #9c6363, #ffffef
